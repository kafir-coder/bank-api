{
   "name": "bank-api",
   "version": "0.0.1",
   "description": "A simple API that implements trivial bank account operation",
   "main": "index.js",
   "scripts": {
      "lint": "eslint src --ext .ts",
      "test": "jest --passWithNoTests --runInBand --no-cache",
    "test:unit": "npm test -- --watch -c jest-unit-config.js",
    "test:integration": "npm test -- --watch -c jest-integration-config.js",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm test -- --coverage",
      "prebuild": "rimraf build/",
      "build": "npm run prebuild && tsc",
      "start": "node build/src/index"
   },
   "repository": {
      "type": "git",
      "url": "git+https://github.com/kafir-coder/bank-api.git"
   },
   "keywords": [
      "bank",
      "mutual"
   ],
   "author": "Caio Tony <caiotony16@gmail.com>",
   "license": "MIT",
   "bugs": {
      "url": "https://github.com/kafir-coder/bank-api/issues"
   },
   "homepage": "https://github.com/kafir-coder/bank-api#readme",
   "devDependencies": {
      "@babel/core": "^7.17.7",
      "@babel/preset-env": "^7.16.11",
      "@commitlint/cli": "^16.2.3",
      "@commitlint/config-conventional": "^16.2.1",
      "@shelf/jest-mongodb": "^2.2.1",
      "@types/jest": "^27.4.1",
      "@types/node": "^17.0.21",
      "@typescript-eslint/eslint-plugin": "^5.15.0",
      "@typescript-eslint/parser": "^5.15.0",
      "babel-jest": "^27.5.1",
      "eslint": "^8.11.0",
      "husky": "^7.0.4",
      "jest": "^27.5.1",
      "ts-jest": "^27.1.3",
      "typescript": "^4.6.2",
      "@types/mongoose": "^5.11.97"
   },
   "dependencies": {
      "@tsclean/core": "^1.7.0",
      "mongodb": "^4.4.1",
      "mongoose": "^6.0.10",
      "rimraf": "^3.0.2"
   }
}